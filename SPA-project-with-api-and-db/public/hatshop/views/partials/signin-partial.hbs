<div class="container">
    <form>
        <div class="form-group">
            <label for="inputEmail" class="form-text">Enter your email</label>
            <input class="form-control" type="email" id="inputEmail" placeholder="Email">
        </div>
        <div class="form-group">
            <label for="inputPassword" class="form-text">Enter your password</label>
            <input class="form-control" type="password" id="inputPassword" placeholder="Password">
        </div>
        <div class="col-md-12">
            <a class="float-left" href="index" onclick="Controller.controllers.signin.goToIndex_clicked(event)" >Go to Index</a>
            <button class="btn btn-primary float-right" onclick="Controller.controllers.signin.signIn(event)" value="Click me">Sign In</button>
        </div>
    </form>
</div>
